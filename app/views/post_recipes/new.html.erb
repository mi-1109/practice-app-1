<h1>POST RECIPE</h1>
<p>レシピを投稿しよう！</p>

<%= form_with model: @post_recipe, local:true do |f| %>
  <h4>レシピ画像</h4>
  <%= f.attachment_field :recipe_image %>
  <h4>レシピ名</h4>
  <%= f.text_field :title %>
  <h4>レシピ説明</h4>
  <%= f.text_field :caption %>
  <h4>材料</h4>
  <h4>手順</h4>
  <%= f.submit 'レシピを投稿' %>
<% end %>
